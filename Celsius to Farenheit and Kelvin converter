using System;
					
public class Program
{
	public static void Main()
	{
		int x;
		Console.WriteLine("Celsius converter");
		Console.WriteLine("Insert temperature in Celsius");
		x = Convert.ToInt32(Console.ReadLine());
		Console.WriteLine("Your temperature in Farenheit: " + (x * 18 / 10 + 32));
		Console.WriteLine("Your temperature in Kelvin: " + (x + 273));
	}
}
