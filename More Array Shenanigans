using System;
					
public class Program
{
	public static void Main()
	{
	int[] array = { 0, 1, 3, 4, 5, 2, 1, -4, -1, 10, 55 };
	int x = 44;
	int[] newarray = new int[array.Length + 1];
	
	for(int i = 0; i < array.Length; i++)
	{	
		int j = 0;
		if(j == 4)
			continue;
		j++;
			
		newarray[i] = array[j];
		newarray[4] = x;
		array = newarray;
	}	
	for(int i = 0; i< array.Length; i++)
		{
			Console.WriteLine(array[i]);
		}
	}
}
