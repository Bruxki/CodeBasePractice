using System;
					
public class Program
{
	public static void Main()
	{
		int x;
		Console.WriteLine("Rectangle maker");
		Console.WriteLine("\nInsert a number");
		x = Convert.ToInt32(Console.ReadLine());
		Console.WriteLine($"\n{x}{x}{x}\n{x} {x}\n{x} {x}\n{x} {x}\n{x} {x}\n{x} {x}\n{x}{x}{x}");
		
	}
}
